{
    "linter": {
        "rules": {
            "suspicious": {
                "noPrototypeBuiltins": "off"
            },
            "style": {
                "useImportType": "off",
                "noNonNullAssertion": "off"
            },
            "correctness": {
                "noUnusedImports": "off",
                "useExhaustiveDependencies": "off"
            },
            "complexity": {
                "noForEach": "off"
            }
        }
    },
    "javascript": {
        "parser": {
            "unsafeParameterDecoratorsEnabled": true
        }
    }
}